{
    "name": "aws-profile-containers",
    "version": "2.0.0",
    "description": "Firefox extension to open AWS profiles from credentials file in separate containers",
    "main": "index.js",
    "scripts": {
        "build:transpile": "webpack --config webpack.prod.js",
        "build:package": "web-ext build --source-dir ./dist/",
        "build": "run-s build:transpile build:package",
        "dev": "webpack -w --config webpack.dev.js",
        "test": "jest --config=jest.config.js",
        "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
        "prettify": "prettier --write \"src/**/*.ts*\"",
        "start:firefox": "web-ext run --source-dir ./dist/"
    },
    "keywords": [
        "firefox",
        "extension",
        "aws",
        "containers",
        "credentials",
        "profiles",
        "isolation",
        "native-messaging"
    ],
    "author": "Sam Fakhreddine",
    "license": "MIT",
    "homepage": "https://github.com/sam-fakhreddine/granted-containers",
    "repository": {
        "type": "git",
        "url": "https://github.com/sam-fakhreddine/granted-containers"
    },
    "devDependencies": {
        "@babel/core": "^7.24.0",
        "@babel/preset-env": "^7.24.0",
        "@babel/preset-typescript": "^7.24.0",
        "@types/chrome": "^0.0.268",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.0",
        "@types/react": "^18.2.79",
        "@types/react-dom": "^18.2.25",
        "@typescript-eslint/eslint-plugin": "^7.5.0",
        "@typescript-eslint/parser": "^7.5.0",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "css-loader": "^7.1.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.1",
        "jest": "^29.7.0",
        "jest-css-modules": "^2.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.2.5",
        "sass-loader": "^14.2.1",
        "style-loader": "^4.0.0",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.5",
        "web-ext": "^8.0.0",
        "webextension-polyfill-ts": "^0.22.0",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-merge": "^5.10.0"
    },
    "dependencies": {
        "bootstrap": "^4.5.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-test-renderer": "^16.13.1",
        "sass": "^1.49.8"
    }
}
