{
  "name": "aws-profile-containers",
  "version": "0.1.0",
  "private": true,
  "description": "Firefox extension to open AWS profiles from credentials file in separate containers",
  "main": "index.js",
  "scripts": {
    "preinstall": "node scripts/check-node-version.js",
    "build:optimize-images": "node scripts/optimize-images.js",
    "build:update-version": "node scripts/build/update-version.js",
    "build:transpile": "webpack --config config/webpack/webpack.prod.js",
    "build:package": "web-ext build --source-dir ./dist/ --overwrite-dest",
    "build": "run-s build:optimize-images build:update-version build:transpile build:package",
    "build:analyze": "ANALYZE=true npm run build:transpile",
    "dev": "webpack -w --config config/webpack/webpack.dev.js",
    "test": "jest",
    "test:perf": "jest --testNamePattern=\"Performance\" --verbose",
    "test:perf:watch": "jest --testNamePattern=\"Performance\" --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report playwright-report",
    "test:all": "npm test && npm run test:e2e",
    "lint": "eslint --fix \"src/**/*.ts*\"",
    "prettify": "prettier --write \"src/**/*.ts*\" --config config/.prettierrc.js",
    "start:firefox": "web-ext run --source-dir ./dist/",
    "semantic-release": "semantic-release",
    "prepare": "husky"
  },
  "keywords": [
    "firefox",
    "extension",
    "aws",
    "containers",
    "credentials",
    "profiles",
    "isolation",
    "api"
  ],
  "author": "Sam Fakhreddine",
  "license": "MIT",
  "homepage": "https://github.com/sam-fakhreddine/aws-console-containers",
  "repository": {
    "type": "git",
    "url": "https://github.com/sam-fakhreddine/aws-console-containers"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@playwright/test": "^1.49.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "^0.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.2",
    "@types/webextension-polyfill": "^0.12.4",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.0.0",
    "jest": "^30.2.0",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^30.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "semantic-release": "^25.0.2",
    "semver": "^7.6.0",
    "sharp": "^0.34.5",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "web-ext": "^9.1.0",
    "webpack": "^5.95.0",
    "webpack-bundle-analyzer": "^5.0.0",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@cloudscape-design/components": "^3.0.1136",
    "@cloudscape-design/design-tokens": "^3.0.64",
    "@cloudscape-design/global-styles": "^1.0.48",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-test-renderer": "^19.2.0",
    "webextension-polyfill": "^0.12.0"
  }
}
