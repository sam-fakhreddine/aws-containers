{
  "name": "aws-profile-containers",
  "version": "0.1.0",
  "description": "Firefox extension to open AWS profiles from credentials file in separate containers",
  "main": "index.js",
  "scripts": {
    "build:transpile": "webpack --config webpack.prod.js",
    "build:update-version": "node scripts/update-version.js",
    "build:package": "web-ext build --source-dir ./dist/ --overwrite-dest",
    "build": "run-s build:transpile build:update-version build:package",
    "dev": "webpack -w --config webpack.dev.js",
    "test": "jest --config=jest.config.js",
    "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
    "prettify": "prettier --write \"src/**/*.ts*\"",
    "start:firefox": "web-ext run --source-dir ./dist/",
    "semantic-release": "semantic-release",
    "prepare": "husky"
  },
  "keywords": [
    "firefox",
    "extension",
    "aws",
    "containers",
    "credentials",
    "profiles",
    "isolation",
    "native-messaging"
  ],
  "author": "Sam Fakhreddine",
  "license": "MIT",
  "homepage": "https://github.com/sam-fakhreddine/granted-containers",
  "repository": {
    "type": "git",
    "url": "https://github.com/sam-fakhreddine/granted-containers"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@types/chrome": "^0.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/webextension-polyfill": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^24.0.0",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "husky": "^9.0.0",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^30.2.0",
    "jest-css-modules": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "web-ext": "^9.1.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-test-renderer": "^19.2.0",
    "sass": "^1.80.0",
    "webextension-polyfill": "^0.12.0"
  }
}
