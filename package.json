{
    "name": "aws-profile-containers",
    "version": "2.0.0",
    "description": "Firefox extension to open AWS profiles from credentials file in separate containers",
    "main": "index.js",
    "scripts": {
        "build:transpile": "webpack --config webpack.prod.js",
        "build:package": "web-ext build --source-dir ./dist/",
        "build": "run-s build:transpile build:package",
        "dev": "webpack -w --config webpack.dev.js",
        "test": "jest --config=jest.config.js",
        "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
        "prettify": "prettier --write \"src/**/*.ts*\"",
        "start:firefox": "web-ext run --source-dir ./dist/"
    },
    "keywords": [
        "firefox",
        "extension",
        "aws",
        "containers",
        "credentials",
        "profiles",
        "isolation",
        "native-messaging"
    ],
    "author": "Sam Fakhreddine",
    "license": "MIT",
    "homepage": "https://github.com/sam-fakhreddine/granted-containers",
    "repository": {
        "type": "git",
        "url": "https://github.com/sam-fakhreddine/granted-containers"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-typescript": "^7.10.4",
        "@types/chrome": "^0.0.124",
        "@types/jest": "^26.0.14",
        "@types/node": "^14.11.8",
        "@types/react": "^16.9.52",
        "@types/react-dom": "^16.9.8",
        "@typescript-eslint/eslint-plugin": "^4.4.1",
        "@typescript-eslint/parser": "^4.4.1",
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "^6.26.3",
        "babel-jest": "^26.5.2",
        "babel-loader": "^8.1.0",
        "css-loader": "^4.3.0",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.4",
        "jest": "^26.5.3",
        "jest-css-modules": "^2.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.2",
        "sass-loader": "^10.0.3",
        "style-loader": "^2.0.0",
        "ts-jest": "^26.4.1",
        "ts-loader": "^8.0.5",
        "typescript": "^4.0.3",
        "web-ext": "^6.6.0",
        "webextension-polyfill-ts": "^0.20.0",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "bootstrap": "^4.5.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-test-renderer": "^16.13.1",
        "sass": "^1.49.8"
    }
}
